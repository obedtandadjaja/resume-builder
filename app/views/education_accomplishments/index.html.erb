<p id="notice"><%= notice %></p>

<h1>Listing Education Accomplishments</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Education</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @education_accomplishments.each do |education_accomplishment| %>
      <tr>
        <td><%= education_accomplishment.description %></td>
        <td><%= education_accomplishment.education %></td>
        <td><%= link_to 'Show', education_accomplishment %></td>
        <td><%= link_to 'Edit', edit_education_accomplishment_path(education_accomplishment) %></td>
        <td><%= link_to 'Destroy', education_accomplishment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Education accomplishment', new_education_accomplishment_path %>
